{
    "version": "1.0",
    "examples": [
        {
            "name": "Integration Test",
            "type": "integration",
            "file_path": "./server/some/file.test.ts",
            "description": "Integration test with database setup"
        },
        {
            "name": "Basic Unit Test",
            "type": "unit",
            "file_path": "./server/some/file.test.ts",
