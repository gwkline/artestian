            },
            {
                "path": "./utils/test-helpers.ts",
                "description": "Common test utilities and helper functions",
                "type": "utils"
            },
            {
                "path": "./constants/test-data.ts",
                "description": "Test data constants and fixtures",
                "type": "constants"
            }
        ]
    }
}